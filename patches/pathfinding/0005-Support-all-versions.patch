Subject: [PATCH] Support all versions


diff --git a/src/main/java/pathfinding/platform/Location.java b/src/main/java/pathfinding/platform/Location.java
index ae6c69c5bb04231d1fda7dec2df88cd9bd30e203..99c79985f2e05905845401e8b6a677391077ce3d 100644
--- a/src/main/java/pathfinding/platform/Location.java
+++ b/src/main/java/pathfinding/platform/Location.java
@@ -119,7 +119,12 @@ public class Location implements Cloneable {
 
     public boolean isSolidBlock() {
 
-        return Block.isShapeFullBlock(this.getBLockState().getOcclusionShape());
+        return Block.isShapeFullBlock(this.getBLockState().getOcclusionShape(
+                #if MC_VER <= MC_1_21_0
+                level,
+                new BlockPos(getX(), getY(), getZ())
+                #endif
+        ));
 
     }
 
